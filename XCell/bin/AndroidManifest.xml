<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jemcphe.xcell"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.Xcell" >
        <activity
            android:name="com.jemcphe.xcell.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity 
            android:name="com.jemcphe.xcell.FragmentAdds"
            android:parentActivityName="com.jemcphe.xcell.MainActivity">
        </activity> 
        
        <activity 
            android:name="com.jemcphe.xcell.MyDataActivity"
            android:parentActivityName="com.jemcphe.xcell.MainActivity">
        </activity>
    </application>

</manifest>
